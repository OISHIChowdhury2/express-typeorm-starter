{
  "name": "express-typeorm-starter-next",
  "version": "1.0.0",
  "description": "Express TypeORM Boilerplate with TypeScript.",
  "main": "./dist/server.js",
  "repository": "https://ovichowdhury@github.com/ovichowdhury/express-typeorm-starter.git",
  "author": "Nahid Chowdhury",
  "license": "MIT",
  "engines": {
    "node": ">=16 <17"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=./dist && cross-env NODE_ENV=production && node --unhandled-rejections=strict ./dist/server.js",
    "dev": "cross-env DEBUG=* && cross-env NODE_ENV=development && nodemon ./src/server.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "husky": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.6.3",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
